{
  "manifest_version": 2,
  "name": "New Tab Replacement v2",
  "description": "Personalized new tab page for Chrome.",
  "version": "2.0.0",
  "offline_enabled": true,
  "options_page": "index.html",
  "permissions": [
    "geolocation",
    "*://*.hko.gov.hk/*",
    "*://*.weather.com.cn/*"
  ],
  "background": {
    "scripts": [
      "dist/serv/vendor.js",
      "dist/serv/background.js"
    ]
  }
}
