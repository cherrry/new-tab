{
  "manifest_version": 2,
  "name": "New Tab Replacement",
  "description": "Personalized new tab page for Chrome.",
  "version": "2.0.0",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "geolocation",
    "*://*.hko.gov.hk/*",
    "*://*.weather.com.cn/*"
  ],
  "background": {
    "scripts": [
      "dist/common.js",
      "dist/background.js"
    ]
  }
}
