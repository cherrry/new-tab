{
  "manifest_version": 2,
  "name": "New Tab Replacement",
  "description": "Personalized new tab page for Chrome.",
  "version": "2.0.0",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "app.html"
  },
  "permissions": [
    "geolocation",
    "http://*.hko.gov.hk/*",
    "http://*.weather.com.cn/*",
    "http://api.openweathermap.org/*"
  ],
  "background": {
    "scripts": [
      "assets/common.js",
      "assets/serv.js"
    ]
  }
}
